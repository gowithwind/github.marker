{
    "manifest_version": 3,
    "name": "Github marker",
    "version": "1.0",
    "description": "Github marker",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Github marker",
        "default_icon": "img/icon_72.png"
      },    
    "permissions": [
        "activeTab"
    ]
}